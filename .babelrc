{
	"presets": ["@babel/preset-env", "@babel/preset-typescript"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["src"],
				"alias": {
					"models": "./src/models",
					"providers": "./src/providers",
					"database": "./src/database",
					"utils": "./src/utils",
					"middlewares": "./src/middlewares",
					"controllers": "./src/controllers"
				}
			}
		]
	]
}
